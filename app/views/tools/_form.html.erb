<%= simple_form_for(@tool) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>
  <div class="form-inputs">
    <%= f.input :symbol %>
  </div>
  <div class="form-inputs">
    <%= f.input :avatar %>
  </div>
  
  <div class="form-inputs">
    <%= f.association :traderoom %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
